<!-- Header -->
<header id="" class="header-landing">
    <div class="container text-center">
    	<div class="container text-center">
    		<%= image_tag "logo.png", :class => "logo-prelaunch", :size => "175x123" %>
		</div>
    	<div class="header-landing-text">
    		<% if @n.blank? %>
    			<% if @aventurier.commentaire.blank? %>
    				<h1 class="header-first-text">L'aventure Switchiz commence bientôt</h1>
    			<% else %>
    				<h1 class="header-first-text"><%= @aventurier.commentaire %> t'invite à découvrir Switchiz</h1>
    			<% end %>
    		<% else %>
    			<h1 class="header-first-text">Hello <%= @n %></h1>
    		<% end %>
    		
			<h2 class="header-next-text">Prépare-Toi à Sourire 7 Fois Plus Qu'avant !</h2>
    	</div>
	</div>

<%= render 'modalone' %>
</header>

<section class="section-landing">
	
	
	
		<br>
	<i class="fa fa-lock fa-2x" aria-hidden="true"></i>
	<h3>Ton smartphone sera bientôt entre de bonnes mains ;) </h3><h3> Fait partie des premiers au courant ! </h3>
	<%= render 'form' %>

	<div class="col-md-12 text-center"><br>
		<p>Copyright Switchiz 2016 - <a href="mailto:gwenael@switchiz.co">gwenael@switchiz.co</a></p>
	</div>
</section>


