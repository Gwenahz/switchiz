<%= form_for(@profil) do |f| %>
  <% if @profil.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@profil.errors.count, "error") %> prohibited this profil from being saved:</h2>

      <ul>
      <% @profil.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="col-md-12">
    <div class="form-group col-md-6">
      <%= f.label :prenom %><br>
      <%= f.text_field :prenom, class: "form-control" %>
    </div>
    <div class="form-group col-md-6">
      <%= f.label :nom %><br>
      <%= f.text_field :nom, class: "form-control" %>
    </div>
    <div class="form-group col-md-12">
      <%= f.label :adresse %><br>
      <%= f.text_field :adresse, class: "form-control" %>
    </div>
    <div class="form-group col-md-12">
      <%= f.label "Complément d'adresse" %><br>
      <%= f.text_field :compl_adresse, class: "form-control", placeholder: "(Code d'accès, bâtiment, etc...)" %>
    </div>
    <div class="form-group col-md-12">
      <%= f.label "Numéro de téléphone" %><br>
      <%= f.text_field :numtel, class: "form-control" %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
  <div>
<% end %>

<script type="text/javascript">
  function getmodele(modele) {
      document.getElementById('profil_modele').value = modele;
      return false;
  }
</script>
